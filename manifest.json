{
    "name": "Getting Started Example",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "background": "background.js",
    "permissions": ["storage", "activeTab", "scripting"],
    "action": {
        "name": "Snow?",
        "default_popup": "popup.html"
      },
    "content_scripts": [ 
        { 
            "js": [ "jquery.js", "jquery-ui.js", "scripts.js" ], 
            "matches": [ "http://*/*", "https://*/*"] 
        }
    ]
  }