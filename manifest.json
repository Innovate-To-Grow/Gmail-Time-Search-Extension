{
    "name": "Gmail Date Filter",
    "description": "Used to make a date filter for gmail",
    "version": "1.0",
    "manifest_version": 3,
    "background": "background.js",
    "permissions": ["storage", "activeTab", "scripting"],
    "action": {
        "name": "Snow?",
        "default_popup": "popup.html"
      },
    "content_scripts": [ 
        { 
            "js": [ "jquery.js", "scripts.js", "flatpickr.js" ], 
            "matches": [ "http://*/*", "https://*/*"] 
        }
    ]
  }